<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1"/>
        <css src="css/style.css"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block name="editorial.wrapper" class="Magento\Framework\View\Element\Template"
                   template="Magento_Cms::editor-wrapper.phtml">
                <arguments>
                    <argument name="title_string" xsi:type="string">editorial</argument>
                </arguments>
                <block name="first.child" class="Magento\Framework\View\Element\Template" template="Magento_Cms::section-template.phtml">
                   <!-- <arguments>
                        <argument></argument>
                    </arguments>-->
                </block>
                <block name="second.child" class="Magento\Framework\View\Element\Template" template="Magento_Cms::section-template.phtml"></block>
                <block name="third.child" class="Magento\Framework\View\Element\Template" template="Magento_Cms::section-template.phtml"></block>
                <block name="fourth.child" class="Magento\Framework\View\Element\Template" template="Magento_Cms::section-template.phtml"></block>
                <block name="fifth.child" class="Magento\Framework\View\Element\Template" template="Magento_Cms::section-template.phtml"></block>
            </block>
        </referenceContainer>
    </body>
</page>
